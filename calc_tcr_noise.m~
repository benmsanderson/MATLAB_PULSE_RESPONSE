function [tcr,tcr_140,t]=calc_tcr_noise(p,d,ta)
  sfcg=d+5.35*log([1.01.^([1:150])])';;
    dfcg=d*ones(ta,1);
  fcg=[dfcg;sfcg];

%  p_t=p(8:16);
  to=t_rsp(p,fcg);
  t=to-mean(to((ta-99):ta));
  tcr=t(ta+70)-t(ta-50);
  tcr_140=t(ta+140)-t(ta-50);
  tcr_10=mean(t(1:10));
				%  r=-r_rsp(p_t,fcg);

%  b=regress(t,[r,ones(size(r))]);

%  s=b(2);
