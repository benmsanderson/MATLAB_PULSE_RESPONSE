addpath('gwmcmc/')
sdp=[1,2,3,5,6,7,8,9,10,11,12,13];

figure(4)
ecornerplot(mdlpst(sdp,:),'names',pname(sdp))

set(gcf, 'PaperPosition', [0 0 10 10]);
set(gcf, 'PaperSize', [10 10]); 
print(gcf,'-dpdf','-painters',['corner_unbiased.pdf'])


figure(5)
ecornerplot(mdlpstcf(sdp,:),'names',pname(sdp))

set(gcf, 'PaperPosition', [0 0 10 10]);
set(gcf, 'PaperSize', [10 10]); 
print(gcf,'-dpdf','-painters',['corner_paleo.pdf'])

figure(5)
ecornerplot(mdlpstml(sdp,:),'names',pname(sdp))

set(gcf, 'PaperPosition', [0 0 10 10]);
set(gcf, 'PaperSize', [10 10]); 
print(gcf,'-dpdf','-painters',['corner_millar.pdf'])

figure(5)
ecornerplot(mdlpstnl(sdp,:),'names',pname(sdp))

set(gcf, 'PaperPosition', [0 0 10 10]);
set(gcf, 'PaperSize', [10 10]); 
print(gcf,'-dpdf','-painters',['corner_heat.pdf'])
