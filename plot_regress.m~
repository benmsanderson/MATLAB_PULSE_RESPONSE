mdlpstr=(mdlpst(:,:,end)-repmat(mean(mdlpst(:,:,end)',1),400,1)')'./repmat(std(mdlpst(:,:,end)'),400,1);

br=regress(tcrmat',mdlpstr(:,8:end))        ;

subplot(1,6,1)
bar(br)

br=regress(tcr4mat',mdlpstr(:,8:end))        ;

subplot(1,6,2)
bar(br)


br=regress(gregmat',mdlpstr(:,8:end))        ;

subplot(1,6,3)
bar(br)

br=regress(tcrmat',mdlpstr(:,8:end))        ;

subplot(1,6,4)
bar(br)

regress(stmat(:,150)',mdlpstr(:,8:end))


subplot(1,6,5)
bar(br)

regress(tomat85(:,336)',mdlpstr(:,8:end))

subplot(1,6,6)
bar(br)

